<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8"
            fx:controller="de.thatsich.openfx.errorgeneration.intern.control.ErrorDisplayPresenter">
	<content>
		<StackPane>
			<children>
				<Group>
					<children>
						<ImageView fx:id="nodeImageViewError" preserveRatio="true"/>
					</children>
				</Group>
			</children>
		</StackPane>
	</content>
</ScrollPane>
