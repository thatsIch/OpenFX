<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="de.thatsich.openfx.prediction.intern.control.PredictionListPresenter">
    <children>

        <TableView fx:id="nodeTableViewBinaryPredictionList"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

            <columns>
                <TableColumn editable="false" prefWidth="100.0"
                             sortable="false" text="Class" fx:id="nodeTableColumnClassifierName"/>
                <TableColumn editable="false" prefWidth="100.0"
                             sortable="false" text="Extractor" fx:id="nodeTableColumnExtractorName"/>
                <TableColumn editable="false" prefWidth="100.0"
                             sortable="false" text="Frame Size" fx:id="nodeTableColumnFrameSize"/>
                <TableColumn editable="false" prefWidth="100.0"
                             sortable="false" text="Label" fx:id="nodeTableColumnErrorClassName"/>
                <TableColumn editable="false" prefWidth="100.0"
                             sortable="false" text="ID" fx:id="nodeTableColumnID"/>
            </columns>

            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>

        </TableView>

    </children>
</AnchorPane>