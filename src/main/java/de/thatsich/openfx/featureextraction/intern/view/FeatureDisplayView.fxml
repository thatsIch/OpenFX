<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fitToHeight="true" fitToWidth="true" prefHeight="100.0"
            prefWidth="100.0"
            xmlns="http://javafx.com/javafx/8"
            fx:controller="de.thatsich.openfx.featureextraction.intern.control.FeatureDisplayPresenter">
	<StackPane>
		<Group>
			<GridPane>
				<columnConstraints>
					<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
					<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
				</columnConstraints>
				<rowConstraints>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
					<RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
				</rowConstraints>

				<Label text="ClassName" GridPane.columnIndex="0" GridPane.rowIndex="0">
					<font>
						<Font name="System Bold" size="11.0" fx:id="x1"/>
					</font>
				</Label>
				<Label font="$x1" text="ExtractorName" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
				<Label font="$x1" text="FrameSize" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
				<Label font="$x1" text="ID" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
				<Label font="$x1" text="FeatureVector" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
				<Label font="$x1" text="FeatureLabel" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
				<Label fx:id="nodeLabelClassName" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="0"/>
				<Label fx:id="nodeLabelExtractorName" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="1"/>
				<Label fx:id="nodeLabelFrameSize" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="2"/>
				<Label fx:id="nodeLabelSize" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="3"/>
				<Label fx:id="nodeLabelFeatureVector" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="4"/>
				<Label fx:id="nodeLabelFeatureLabel" text="" GridPane.columnIndex="1"
				       GridPane.rowIndex="5"/>
			</GridPane>
		</Group>
	</StackPane>
</ScrollPane>
