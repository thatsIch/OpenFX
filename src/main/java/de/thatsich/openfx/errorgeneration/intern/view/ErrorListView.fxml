<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="de.thatsich.openfx.errorgeneration.intern.control.ErrorListPresenter">

	<TableView fx:id="nodeTableViewErrorList"
	           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
	           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
		<columns>
			<TableColumn editable="false" minWidth="100" prefWidth="100.0"
			             sortable="false" text="Class" fx:id="nodeTableColumnErrorClass"/>
		</columns>

		<columnResizePolicy>
			<TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
		</columnResizePolicy>
	</TableView>

</AnchorPane>
