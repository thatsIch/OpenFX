<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.thatsich.bachelor.javafx.presentation.error.ErrorInputPresenter">
  <children>
    <Label text="Error Generator" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <labelFor>
        <ChoiceBox id="ChoiceBoxErrorGenerator" fx:id="nodeChoiceBoxErrorGenerator" minWidth="25.0" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      </labelFor>
    </Label>
    <fx:reference source="nodeChoiceBoxErrorGenerator" />
    <TextField fx:id="nodeTextFieldErrorCount" alignment="CENTER_RIGHT" prefWidth="200.0" text="1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Label labelFor="$nodeTextFieldErrorCount" text="Create Error Count" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button fx:id="nodeButtonPermutateErrors" mnemonicParsing="false" onAction="#onPermutateErrorsAction" prefWidth="125.0" text="Permutate Errors" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Button fx:id="nodeButtonGenerateErrors" mnemonicParsing="false" onAction="#onGenerateErrorsAction" prefWidth="125.0" text="Generate Errors" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Button fx:id="nodeButtonRemoveError" mnemonicParsing="false" onAction="#onRemoveErrorAction" prefWidth="125.0" text="Remove Error" GridPane.columnIndex="0" GridPane.rowIndex="4" />
    <Button fx:id="nodeButtonResetErrors" mnemonicParsing="false" onAction="#onResetErrorsAction" prefWidth="125.0" text="Reset Errors" GridPane.columnIndex="1" GridPane.rowIndex="4" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <padding>
    <Insets fx:id="x2" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="-1.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="-1.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
