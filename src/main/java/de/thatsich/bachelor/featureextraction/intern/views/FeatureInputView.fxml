<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import de.thatsich.core.javafx.component.IntegerField?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.thatsich.bachelor.featureextraction.intern.controller.FeatureInputPresenter">
  <children>
  	<IntegerField fx:id="nodeIntegerFieldFrameSize" alignment="CENTER_RIGHT" prefWidth="200.0" text="1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Label labelFor="$nodeIntegerFieldFrameSize" text="TileSize (PX x PX)" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Label text="Feature Extractor" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <labelFor>
        <ChoiceBox id="ChoiceBoxFeatureExtractor" fx:id="nodeChoiceBoxFeatureExtractor" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      </labelFor>
    </Label>
    <fx:reference source="nodeChoiceBoxFeatureExtractor" />
    <Button fx:id="nodeButtonRemoveFeatureVector" mnemonicParsing="false" onAction="#onRemoveAction" prefWidth="125.00009999999747" text="Remove" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Button fx:id="nodeButtonExtractFeatureVector" mnemonicParsing="false" onAction="#onExtractAction" prefWidth="125.00009999999747" text="Extract" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Button fx:id="nodeButtonResetFeatureVectorList" mnemonicParsing="false" onAction="#onResetAction" prefWidth="125.00009999999747" text="Reset" GridPane.columnIndex="1" GridPane.rowIndex="4" /> 
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
